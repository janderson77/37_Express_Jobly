{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "handle": "koi",
            "name": "Koi Game Mods",
            "num_employees": 12,
            "description": "Video Game Mod Hosting",
            "logo_url": "https://i.pinimg.com/originals/cc/5f/62/cc5f62b925b6de4fc98329acd8676463.png"
        }
    ],
    "required": [
        
    ],
    "properties": {
        "handle": {
            "$id": "#/properties/handle",
            "type": "string",
            "title": "The handle schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "koi"
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Koi Game Mods"
            ]
        },
        "num_employees": {
            "$id": "#/properties/num_employees",
            "type": "integer",
            "title": "The num_employees schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                12
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Video Game Mod Hosting"
            ]
        },
        "logo_url": {
            "$id": "#/properties/logo_url",
            "type": "string",
            "title": "The logo_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "https://i.pinimg.com/originals/cc/5f/62/cc5f62b925b6de4fc98329acd8676463.png"
            ],
            "pattern": "^(?!mailto:)(?:(?:http|https|ftp)://)(?:\\S+(?::\\S*)?@)?(?:(?:(?:[1-9]\\d?|1\\d\\d|2[01]\\d|22[0-3])(?:\\.(?:1?\\d{1,2}|2[0-4]\\d|25[0-5])){2}(?:\\.(?:[0-9]\\d?|1\\d\\d|2[0-4]\\d|25[0-4]))|(?:(?:[a-z\\u00a1-\\uffff0-9]+-?)*[a-z\\u00a1-\\uffff0-9]+)(?:\\.(?:[a-z\\u00a1-\\uffff0-9]+-?)*[a-z\\u00a1-\\uffff0-9]+)*(?:\\.(?:[a-z\\u00a1-\\uffff]{2,})))|localhost)(?::\\d{2,5})?(?:(/|\\?|#)[^\\s]*)?$",
            "format": "uri"
        }
    },
    "additionalProperties": true
}